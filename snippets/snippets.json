{
    "za-loading": {
        "prefix": "zaLoading",
        "body": [
            "<za-loading :visible.sync=''>${1:content}</za-loading>"
        ],
        "description": "zaLoading"
    },
    "za-toast": {
        "prefix": "zaToast",
        "body": [
            "<za-toast:visible.sync='' @close='':duration=''></za-toast>"
        ],
        "description": "zaToast"
    },
    "za-swipe-action": {
        "prefix": "zaSwipeAction",
        "body": [
            "<za-swipe-action:right='${1:right}':left='${2:left}'>",
            "   <za-cell>${3:content}</za-cell>",
            "</za-swipe-action>"
        ],
        "description": "za-swipe-action"
    },
    "za-pull": {
        "prefix": "zaPull",
        "body": [
            "<za-pull :on-refresh='' :refreshing=''>",
            "    <za-cell v-for='i in myData':key='i'>${3:content}</za-cell>",
            "</za-pull>"
        ],
        "description": "zaPull"
    },
    "za-popup": {
        "prefix": "zaPopup",
        "body": [
            "<za-popup:visible.sync='' direction='':duration='' maskType='' @close=''>",
            "    <div>${1:content}</div>",
            "</za-popup>"
        ],
        "description": "zaPopup"
    }
}